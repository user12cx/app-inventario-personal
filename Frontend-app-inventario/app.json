{
  "expo": {
    "name": "app-inventario",
    "slug": "app-inventario",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ssq.png",
    "scheme": "myapp",
    "resizeMode": "contain",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#5A8FCA",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#5A8FCA"
      },
      "package": "com.ivanb19.appinventario"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/auth.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#5A8FCA"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "078eafcc-3bec-4549-b844-2d906e8a757d"
      }
    },
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#5A8FCA",
      "image": "./assets/images/splash-icon.png" 
    }
  }
}
